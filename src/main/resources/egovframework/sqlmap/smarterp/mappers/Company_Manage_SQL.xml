    <select id="selectCompanyList" parameterType="companyVO" resultType="companyVO">
            SELECT
               company_id
	           ,company_nm
	           ,acc_order
	           ,acc_start_d
	           ,acc_end_d
	           ,com_reg_num
	           ,cor_reg_num
	           ,boss_nm
	           ,is_foreigner
	           ,citi_num
	           ,hq_postcode
	           ,hq_addr
	           ,hq_addr_detail
	           ,hq_phone1
	           ,hq_phone2
	           ,hq_fax1
	           ,hq_fax2
	           ,com_biz
	           ,com_cate
	           ,com_type
	           ,com_est_d
	           ,com_open_d
	           ,com_close_d
	           ,is_use
	           ,com_eng_nm
	           ,cor_eng_nm
	           ,com_eng_addr
	           ,com_homepage
	           ,com_gwpage
	           ,home_postcode
	           ,home_addr
	           ,home_addr_detail
	           ,home_phone
	           ,is_deleted
              FROM dbo.company
              WHERE 1=1
				<if test='companyId != null and companyId != "0"'>
				AND company_id = #{companyId}
				</if>
				AND is_deleted = 0
            ORDER BY company_nm
    </select>
    
	<update id="deleteCompanyManage" parameterType="companyVO">
		UPDATE [dbo].[company]
		   SET [is_deleted] = #{isDeleted}
		 WHERE company_id = #{companyId}
	</update>



